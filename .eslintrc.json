{
  "extends": ["next/core-web-vitals", "next/typescript", "prettier"],
  "plugins": [],
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "no-multiple-empty-lines": "warn",
    "no-duplicate-imports": "warn",
    "padding-line-between-statements": [
      "warn",
      {
        "blankLine": "always",
        "prev": "*",
        "next": "return"
      },
      {
        "blankLine": "always",
        "prev": "*",
        "next": "function"
      },
      {
        "blankLine": "always",
        "prev": "*",
        "next": "block-like"
      },
      {
        "blankLine": "always",
        "prev": "block-like",
        "next": "*"
      }
    ]
  }
}
